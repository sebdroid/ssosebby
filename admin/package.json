{
  "name": "admin",
  "version": "0.0.0",
  "scripts": {
    "build": "npm-run-all config tailwind esbuild",
    "dev": "dotenv -- npm-run-all -l -p config serve tailwind-watch esbuild-watch tsc-watch",
    "build-docker": "npm-run-all tailwind esbuild",
    "serve-docker": "npm run config && ./bin/serve-public",
    "config": "./bin/prepare-config-json > ./public/config.json",
    "serve": "http-server -c-1 -p 8083 -P http://localhost:8083?",
    "tailwind": "tailwindcss -i ./src/index.css -o ./public/index.css",
    "tailwind-watch": "tailwindcss -i ./src/index.css -o ./public/index.css --watch",
    "esbuild": "node ./build.mjs",
    "esbuild-watch": "ADMIN_BUILD_IS_DEV=1 node ./build.mjs",
    "tsc": "tsc --noEmit",
    "tsc-watch": "tsc --noEmit --watch --preserveWatchOutput",
    "fmt": "prettier --write .",
    "fmt-check": "prettier --check ."
  },
  "dependencies": {
    "@bufbuild/buf": "^1.55.1",
    "@bufbuild/protobuf": "^1.10.1",
